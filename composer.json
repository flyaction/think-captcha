{
    "name": "flyaction/think-captcha",
    "description": "captcha package for thinkphp，前后端分离，api接口调用",
    "authors": [
        {
            "name": "flyaction",
            "email": "xingdong1117@126.com"
        }
    ],
    "license": "Apache-2.0",
    "require": {
        "topthink/framework": "^6.0.0"
    },
    "autoload": {
        "psr-4": {
            "flyaction\\captcha\\": "src/"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "extra": {
        "think": {
            "services": [
                "flyaction\\captcha\\CaptchaService"
            ],
            "config":{
                "captcha": "src/config.php"
            }
        }
    }
}
